{
  "name": "doggy-ui-v3",
  "description": "A component library for vue3.",
  "author": {
    "name": "Doggy-068",
    "url": "https://github.com/Doggy-068"
  },
  "homepage": "https://doggy-068.github.io/doggy-ui-v3/",
  "repository": {
