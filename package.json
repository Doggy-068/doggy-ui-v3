  },
  "private": false,
  "license": "MIT",
  "version": "1.0.0-0",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --config play/vite.config.ts",
